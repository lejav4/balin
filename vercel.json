{
  "version": 2,
  "builds": [
    {
      "src": "obzg-react-frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/obzg-react-frontend/build/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/obzg-react-frontend/build/index.html"
    }
  ],
  "buildCommand": "cd obzg-react-frontend && npm install && npm run build",
  "outputDirectory": "obzg-react-frontend/build"
} 